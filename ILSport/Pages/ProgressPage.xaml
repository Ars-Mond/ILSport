            <uc:RoundBrick Value="200" MaxValue="100" ImagePath="../Resources/Images/3.png"/>
            <uc:RoundBrick Value="23" MaxValue="53" ImagePath="../Resources/Images/4.png"/>
            <uc:RoundBrick Value="12" MaxValue="22" ImagePath="../Resources/Images/5.png"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
            <uc:RoundBrick Value="0" MaxValue="0"/>
        </c:GapWrapPanel>
    </Grid>
</Page>
